<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SaveMazeController">
   <children>
      <Label layoutX="76.0" layoutY="62.0" text="Enter maze's name to Save">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="btn_saveMaze" layoutX="144.0" layoutY="168.0" mnemonicParsing="false" onAction="#saveMaze" prefHeight="35.0" prefWidth="62.0" text="Save">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="txtfld_mazeName" layoutX="102.0" layoutY="113.0" />
   </children>
</AnchorPane>
